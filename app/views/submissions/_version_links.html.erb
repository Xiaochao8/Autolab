<span id="version-links">
  <% if !@prevVersion.nil? %>
    <%= link_to(url_for([:view, @course, @assessment, @prevVersion[:submission], header_position: @prevVersion[:header_position]]), class: "btn small", title: Sgtn.t('submissions.Previous.version.containing.this.file.shortcut', 'views'), id: "prev_version_link") do %>
      <i class="material-icons">south</i>
    <% end %>
  <% else %>
    <a class="btn disabled"><i class="material-icons">south</i></a>
  <% end %>
  <% if !@nextVersion.nil? %>
    <%= link_to(url_for([:view, @course, @assessment, @nextVersion[:submission], header_position: @nextVersion[:header_position]]), class: "btn small", title: Sgtn.t('submissions.Next.version.containing.this.file.shortcut', 'views'), id: "next_version_link") do %>
      <i class="material-icons">north</i>
    <% end %>
  <% else %>
    <a class="btn disabled"><i class="material-icons">north</i></a>
  <% end %>
</span>
